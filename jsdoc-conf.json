{
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc"]
  },
  "source": {
    "include": ["src"],
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "sourceType": "module",
  "plugins": [
    "./node_modules/tsd-jsdoc/dist/plugin",
    "./node_modules/jsdoc-export-default-interop/dist/index"
  ],
  "opts": {
    "template": "./node_modules/tsd-jsdoc/dist",
    "destination": ".",
    "recurse": true
  }
}
